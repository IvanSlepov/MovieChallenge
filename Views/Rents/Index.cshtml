@model IEnumerable<WebApplication1.Models.Rent>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MovieID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.From)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.To)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @item.User
        </td>
        <td>
            @item.Movie
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.From)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.To)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.RentID }) |
            @Html.ActionLink("Details", "Details", new { id=item.RentID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.RentID })
        </td>
    </tr>
}

</table>
